<Project>
  <PropertyGroup>
    <LangVersion>latest</LangVersion>
	<Suffix Condition="'$(SuffixVersionToVersionInAssembly)' == '-main'"></Suffix>
    <Version>$([System.DateTime]::UtcNow.ToString("yy.MM.dd")).$([System.DateTime]::UtcNow.Subtract($([System.DateTime]::UtcNow.Date)).TotalMilliseconds.ToString("N0"))$(Suffix)</Version>
